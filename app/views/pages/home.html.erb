<header>
  <div class="overlay"></div>
  <video playsinline="playsinline" autoplay="autoplay" muted="muted" loop="loop">
    <source src="https://gcs-vimeo.akamaized.net/exp=1572484013~acl=%2A%2F473935215.mp4%2A~hmac=42345892521e61ccd4192b274affeeaeec4b71d196f7d3dd03f6a88680893b95/vimeo-prod-skyfire-std-us/01/740/6/153703075/473935215.mp4" type="video/mp4">
    </video>
    <div class="container h-100">
      <div class="d-flex h-100 text-center align-items-center">
        <div class="w-100 text-white">
          <h1 class="display-3 text-white">Your best option</h1>
          <p class="lead mb-0 text-dark"></p>
          <%= simple_form_for :search, url: search_path, method: :get do |f| %>
          <div class="search-form-control form-group w-50 mx-auto">
            <select class="form-control select required text-white" name="search[query]" id="search_query">
              <% Car::CATEGORIES.each do |category| %>
              <option value="<%= category %>"><%= category %></option>
              <% end %>
            </select>
            <button name="button" type="submit" class="btn btn-flat text-white">
              <i class="fas fa-search text-white"></i> Search
            </button>
          </div>
          <% end %>
        </div>
      </div>
    </div>
  </header>


  <div class="container mt-5">
    <div class="row">
      <% @cars.first(6).each do |car| %>
      <div class="col-sm-6 col-lg-4">
        <div class="my-3">
          <%= render "cars/car-card", car: car %>
        </div>
      </div>
      <% end %>
    </div>
  </div>



